{
	"line": "none",
	"id": {
		"npm": "{{pkg.name}}",
		"github": "medelman17/svelte-library-template"
	},
	"templates": [],
	"badges": [
		{
			"alt": "NPM VERSION",
			"url": "https://npmjs.com/package/{{ id:npm }}",
			"img": "https://img.shields.io/npm/v/{{ id:npm }}?style=for-the-badge"
		},
		{
			"alt": "NPM DOWNLOADS",
			"url": "https://npmjs.com/package/{{ id:npm }}",
			"img": "https://img.shields.io/npm/dt/{{ id:npm }}?style=for-the-badge"
		},
		{
			"alt": "DEPENDENCIES",
			"url": "https://npmjs.com/package/{{ id:npm }}",
			"img": "https://img.shields.io/librariesio/release/npm/{{ pkg.name }}?style=for-the-badge"
		}
	]
}
